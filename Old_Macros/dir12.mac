! displays direction of stif 12's
PARSAV
!/ANNO,DELE
/REPLO
NELEM
/LSPEC,11
CM,NODES_,NODE
CM,ELES_,ELEM
*GET,NELES,ELEM,,COUNT
ELE=ELNEXT(0)
*DO,LOP,1,NELES
INODE=NELEM(ELE,1)
JNODE=NELEM(ELE,2)
*GET,EREAL,ELEM,ELE,ATTR,REAL
*GET,EANGLE,RCON,EREAL,CONST,1	! get angle
EANGLE=-1*EANGLE
/TSPEC,,.5
XI=NX(INODE)
YI=NY(INODE)
/AN3D,ARRO,.5,XI,YI,0,XI+SIN(EANGLE)*.1,YI+COS(EANGLE)*.1,0,
CMSEL,,ELES_
ELE=ELNEXT(ELE)
*ENDDO
CMSEL,,NODES_
,,ELES_
CMDELE,NODES_
,ELES_
/TSPEC,
/LSPEC,,,,,
PARRES

